{
  "name": "keynote-parser2",
  "version": "0.5.0",
  "description": "A library for parsing Apple Keynote file",
  "keywords": [
    "keynote",
    "keynote-parser",
    "iwa",
    "iwa-parser"
  ],
  "type": "commonjs",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json",
    "./lib/*": "./lib/*",
    "./proto/*": "./proto/*"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "keynote-parser": "./bin/keynote-parser.js"
  },
  "files": [
    "bin",
    "lib",
    "proto"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rimraf lib *.tsbuildinfo",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.cjs,.ts . && prettier --check .",
    "pb": "pnpm pb:js && pnpm pb:ts",
    "pb:js": "pbjs -t static-module -w commonjs -o proto/proto.js proto-files/*.proto",
    "pb:ts": "pbts -o proto/proto.d.ts proto/proto.js",
    "prepare": "husky install",
    "prepublishOnly": "pnpm clean && pnpm build && pnpm lint && pnpm test",
    "publish": "git push origin main --tags",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1 && git add CHANGELOG.md"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,cjs,ts}": "eslint --fix",
    "package.json": "sort-package-json"
  },
  "prettier": "@meteorlxy/prettier-config",
  "dependencies": {
    "decompress": "^4.2.1",
    "fs-extra": "^11.1.1",
    "lodash.get": "^4.4.2",
    "protobufjs": "^7.2.4",
    "snappyjs": "^0.7.0",
    "varint": "^6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@meteorlxy/eslint-config-prettier": "^2.25.0",
    "@meteorlxy/eslint-config-prettier-typescript": "^2.27.1",
    "@meteorlxy/prettier-config": "^2.11.0",
    "@meteorlxy/tsconfig": "^2.23.1",
    "@types/decompress": "^4.2.4",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash.get": "^4.4.7",
    "@types/node": "^20.4.2",
    "@types/varint": "^6.0.1",
    "@vitest/coverage-istanbul": "^0.33.0",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "^8.44.0",
    "fast-glob": "^3.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "long": "^5.2.3",
    "prettier": "^3.0.0",
    "protobufjs-cli": "^1.1.1",
    "rimraf": "^5.0.1",
    "sort-package-json": "^2.5.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vitest": "^0.33.0"
  },
  "packageManager": "pnpm@8.6.7",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint-*"
      ]
    }
  }
}
