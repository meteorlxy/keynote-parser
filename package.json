{
  "name": "keynote-parser2",
  "version": "0.5.0",
  "description": "A library for parsing Apple Keynote file",
  "keywords": [
    "keynote",
    "keynote-parser",
    "iwa",
    "iwa-parser"
  ],
  "type": "commonjs",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json",
    "./lib/*": "./lib/*",
    "./proto/*": "./proto/*"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "keynote-parser": "./bin/keynote-parser.js"
  },
  "files": [
    "bin",
    "lib",
    "proto"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rimraf lib *.tsbuildinfo",
    "format": "prettier --write .",
    "lint": "eslint . && prettier --check .",
    "pb": "pnpm pb:js && pnpm pb:ts",
    "pb:js": "pbjs -t static-module -w commonjs -o proto/proto.js proto-files/*.proto",
    "pb:ts": "pbts -o proto/proto.d.ts proto/proto.js",
    "prepare": "husky",
    "release": "pnpm release:check && pnpm release:version && pnpm release:publish",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:check": "pnpm clean && pnpm build && pnpm lint && pnpm test",
    "release:publish": "pnpm publish",
    "release:version": "bumpp -r --execute=\"pnpm release:changelog\" --commit \"build: publish v%s\" --all",
    "test": "vitest run",
    "test:cov": "vitest run --coverage"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.!(js|ts)": "prettier --write --ignore-unknown",
    "*.(js|ts)": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": "sort-package-json"
  },
  "prettier": "@meteorlxy/prettier-config",
  "dependencies": {
    "decompress": "^4.2.1",
    "fs-extra": "^11.3.0",
    "lodash.get": "^4.4.2",
    "protobufjs": "^7.4.0",
    "snappyjs": "^0.7.0",
    "varint": "^6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@meteorlxy/eslint-config": "^4.8.2",
    "@meteorlxy/prettier-config": "^4.8.0",
    "@meteorlxy/tsconfig": "^4.8.0",
    "@types/decompress": "^4.2.7",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash.get": "^4.4.9",
    "@types/node": "^22.13.1",
    "@types/varint": "^6.0.3",
    "@vitest/coverage-istanbul": "^3.0.5",
    "bumpp": "^10.0.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.19.0",
    "fast-glob": "^3.3.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "long": "^5.2.4",
    "prettier": "^3.4.2",
    "protobufjs-cli": "^1.1.3",
    "rimraf": "^6.0.1",
    "sort-package-json": "^2.14.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.5"
  },
  "packageManager": "pnpm@10.2.0"
}
